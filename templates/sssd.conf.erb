<% @final_config.sort.map do |k,v| -%>
<%   if v.is_a?(Hash) -%>
[<%=  k %>]
<%     v.sort.map do |ki, vi| -%>
<%       if vi.is_a?(Array) -%>
<%         vi.each do |vii| -%>
<%=          ki %> = <%= vii %>
<%         end -%>
<%       elsif vi != :undef -%>
<%=        ki %> = <%= vi %>
<%       end -%>
<%     end -%>
<%   end %>
<% end -%>

